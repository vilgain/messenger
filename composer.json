{
    "name": "vilgain/messenger",
    "description": "Best Symfony Messenger for Nette framework - fork from contributte/messenger",
    "keywords": [
        "nette",
        "symfony",
        "messenger",
        "async",
        "consumer",
        "publisher"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/vilgain/messenger",
    "authors": [
        {
            "name": "Milan Felix Å ulc",
            "homepage": "https://f3l1x.io"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=8.3",
        "nette/di": "^3.2.4",
        "psr/container": "^2.0.2",
        "psr/cache": "^2.0.0 || ^3.0.0",
        "psr/log": "^2.0.0 || ^3.0.0",
        "symfony/messenger": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/console": "^7.3",
        "symfony/var-dumper": "^7.3"
    },
    "require-dev": {
        "psr/container": "^2.0.2",
        "mockery/mockery": "^1.6",
        "symfony/redis-messenger": "^7.3",
        "symfony/amqp-messenger": "^7.3",
        "symfony/doctrine-messenger": "^7.3",
        "contributte/console": "^0.10.0",
        "contributte/event-dispatcher": "^0.9.0",
        "contributte/qa": "^0.4.0",
        "contributte/tester": "^0.3.0",
        "contributte/phpstan": "^0.2.0",
        "tracy/tracy": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Contributte\\Messenger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
